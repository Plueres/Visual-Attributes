{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.chicken": {
      "geometry": "geometry.default",
      "materials": [
        {
          "*": "Material.default"
        },
        {
          "leg*": "Material.legs"
        }
      ],
      "textures": [
        "query.is_in_love ? Texture.in_love : texture.default"
      ]
    },
    "controller.render.cow": {
      "arrays": {
        "textures": {
          "Array.base": [
            "Texture.default",
            "Texture.dairy_cow"
          ],
          "Array.in_love": [
            "Texture.in_love",
            "Texture.dairy_cow_in_love"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "q.is_in_love ? Array.in_love[variable.random] : Array.base[variable.random]"
      ]
    },
    "controller.render.pig": {
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.default",
            "Texture.saddled"
          ],
          "Array.loved": [
            "Texture.in_love",
            "Texture.in_love_saddled"
          ]
        }
      },
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "query.is_in_love ? Array.loved[query.is_saddled] : Array.skins[query.is_saddled]"
      ]
    },
    "controller.render.sheep": {
      "arrays": {
        "geometries": {
          "Array.geos": [
            "Geometry.default",
            "Geometry.sheared"
          ]
        }
      },
      "geometry": "Array.geos[query.is_sheared]",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "query.is_in_love ? Texture.in_love : Texture.default"
      ]
    }
  }
}