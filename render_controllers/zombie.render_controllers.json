{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.zombie": {
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "(query.is_in_water && query.is_shaking) ? Texture.drowned : (query.is_shaking ? Texture.frozen : Texture.default)"
      ]
    }
  }
}