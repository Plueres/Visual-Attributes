{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.werewolf_hairs": {
      "arrays": {
				"textures": {
					"Array.text": [
						"Texture.none",
						"Texture.none",
						"Texture.none",
						"Texture.none",
						"Texture.none",
						"Texture.werewolf"
					]
				}
			},
      "geometry": "Geometry.werewolf",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "query.moon_phase == 0 ? Array.text[v.random] : Texture.default"
      ]
    },
    "controller.render.zombie": {
      "arrays": {
				"textures": {
					"Array.text": [
						"Texture.default",
						"Texture.default",
						"Texture.default",
						"Texture.default",
						"Texture.default",
						"Texture.werewolf"
					]
				}
			},
      "geometry": "Geometry.default",
      "materials": [
        {
          "*": "Material.werewolf"
        }
      ],
      "textures": [
        "query.moon_phase == 0 ? Array.text[v.random] : ((query.is_in_water && query.is_shaking) ? Texture.drowned : (query.is_shaking ? Texture.frozen : Texture.default))"
      ]
    }
  }
}