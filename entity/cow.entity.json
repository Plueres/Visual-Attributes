{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:cow",
      "min_engine_version": "1.8.0",
      "materials": {
        "default": "cow"
      },
      "textures": {
        "default": "textures/entity/cow/cow",
        "in_love": "textures/entity/cow/cow_in_love",
        "dairy_cow": "textures/entity/cow/dairy_cow",
        "dairy_cow_in_love": "textures/entity/cow/dairy_cow_in_love",
        "leashed": "textures/entity/cow/cow_leashed"
      },
      "geometry": {
        "default": "geometry.cow.v1.8"
      },
      "animations": {
        "setup": "animation.cow.setup",
        "walk": "animation.quadruped.walk",
        "look_at_target": "animation.common.look_at_target",
        "baby_transform": "animation.cow.baby_transform"
      },
      "scripts": {
        "scale": "variable.scale_random",
        "initialize": [
          "variable.scale_random = math.random(0.95, 1.05);",
          "variable.random = math.random(0, 1);"
        ],
        "animate": [
          "setup",
          {
            "walk": "query.modified_move_speed"
          },
          "look_at_target",
          {
            "baby_transform": "query.is_baby"
          }
        ]
      },
      "render_controllers": [
        "controller.render.cow"
      ]
    }
  }
}